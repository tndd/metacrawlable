{
  "name": "metacrawlable",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "playwright test",
    "test:static": "PLAYWRIGHT_PROJECT=static-land playwright test --project=static-land",
    "test:dynamic": "PLAYWRIGHT_PROJECT=dynamic-maze playwright test --project=dynamic-maze",
    "test:client": "PLAYWRIGHT_PROJECT=client-shadow playwright test --project=client-shadow",
    "test:antibot": "PLAYWRIGHT_PROJECT=bot-warden playwright test --project=bot-warden",
    "test:report": "npx playwright show-report",
    "test:report:static": "npx playwright show-report $(ls -td tests/static/result/*/reports/html 2>/dev/null | head -1 || echo 'tests/static/result')",
    "test:docker": "docker-compose run --rm playwright-test",
    "test:docker:static": "docker-compose run --rm playwright-test npx playwright test --project=static-land",
    "test:docker:all": "docker-compose up -d app && docker-compose run --rm playwright-test && docker-compose down"
  },
  "dependencies": {
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "typescript": "^5"
  }
}
